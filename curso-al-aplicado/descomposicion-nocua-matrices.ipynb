   "source": [
    "# **¿Cómo descompongo una matriz no cuadrada (SVD)?**"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "A = np.array([[1,2,3],[3,4,5]])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "U: [[-0.46410668 -0.88577931]\n",
      " [-0.88577931  0.46410668]]\n",
      "D: [[7.97638869 0.        ]\n",
      " [0.         0.61418515]]\n",
      "V: [[-0.39133557 -0.5605708  -0.72980603]\n",
      " [ 0.8247362   0.13817999 -0.54837623]\n",
      " [ 0.40824829 -0.81649658  0.40824829]]\n"
     ]
    }
   ],
   "source": [
    "U, D, V = np.linalg.svd(A)\n",
    "print(f'U: {U}')\n",
    "print(f'D: {np.diag(D)}')\n",
    "print(f'V: {V}')"
   ]
  }
